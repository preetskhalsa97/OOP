
public class ThreadDemo {

	public static void main(String[] Args){
		// main thread
		// thread instantiation 
		MyThread t = new MyThread(); //main thread creating a child thread object
		t.start(); //starting the child thread ==> now, two threads are running
		for (int i=0;i<10;i++){
			System.out.println("Main Thread");
		}
	}
}
