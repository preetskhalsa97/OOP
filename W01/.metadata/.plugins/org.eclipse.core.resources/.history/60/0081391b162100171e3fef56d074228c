import javax.swing.JFrame;

public class LeftThread extends Thread{
	
	CircleIcon c; 
	JFrame f; 
	
	LeftThread(CircleIcon c,JFrame f){
		this.c=c; 
		this.f=f; 
	}
	
	boolean suspendFlag; 
	
	public void mySuspend(){
		suspendFlag=true; 
	}
	
	public void myResume(){
		suspendFlag=false; 
	}
	
	
	

}
